{
  "name": "sales-dashboard",
  "version": "1.3.1",
  "description": "A dashboard built upon chart.js for displaying sales transactions",
  "bin": "build/server.js",
  "main": "build/server.js",
  "files": [
    "build/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickp10/sales-dashboard.git"
  },
  "author": "nickp10@hotmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nickp10/sales-dashboard/issues"
  },
  "homepage": "https://github.com/nickp10/sales-dashboard#readme",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-client": "webpack --config webpack.client.config.js --mode production",
    "build-server": "webpack --config webpack.server.config.js --mode production",
    "prepare": "npm run build",
    "start": "node build/server.js"
  },
  "dependencies": {
    "argv": "0.0.2",
    "bootstrap": "^4.5.0",
    "chart.js": "^2.9.3",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "moment": "^2.29.2",
    "mysql": "^2.18.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "@types/argv": "0.0.4",
    "@types/bootstrap": "^4.5.0",
    "@types/chart.js": "^2.9.21",
    "@types/express": "^4.17.6",
    "@types/mysql": "^2.15.13",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-bootstrap": "^1.0.1",
    "@types/react-dom": "^16.9.8",
    "babel": "^6.23.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "css-loader": "^3.5.3",
    "html-webpack-plugin": "^4.3.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}

{
  "name": "sales-dashboard",
  "version": "1.3.1",
  "description": "A dashboard built upon chart.js for displaying sales transactions",
  "bin": "build/server.js",
  "main": "build/server.js",
  "files": [
    "build/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickp10/sales-dashboard.git"
  },
  "author": "nickp10@hotmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nickp10/sales-dashboard/issues"
  },
  "homepage": "https://github.com/nickp10/sales-dashboard#readme",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-client": "webpack --config webpack.client.config.js --mode production",
    "build-server": "webpack --config webpack.server.config.js --mode production",
    "prepare": "npm run build",
    "start": "node build/server.js"
  },
  "dependencies": {
    "argv": "0.0.2",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-chartjs-2": "^2.7.6",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@types/argv": "0.0.4",
    "@types/bootstrap": "^4.3.1",
    "@types/chart.js": "^2.8.5",
    "@types/express": "^4.17.1",
    "@types/mysql": "^2.15.7",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-bootstrap": "^0.32.19",
    "@types/react-dom": "^16.9.0",
    "babel": "^6.23.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "css-loader": "^3.2.0",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.1.0",
    "typescript": "^3.6.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9"
  }
}

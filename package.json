{
  "name": "sales-dashboard",
  "version": "1.3.1",
  "description": "A dashboard built upon chart.js for displaying sales transactions",
  "bin": "build/server.js",
  "main": "build/server.js",
  "files": [
    "build/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickp10/sales-dashboard.git"
  },
  "author": "nickp10@hotmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nickp10/sales-dashboard/issues"
  },
  "homepage": "https://github.com/nickp10/sales-dashboard#readme",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-client": "webpack --config webpack.client.config.js --mode production",
    "build-server": "webpack --config webpack.server.config.js --mode production",
    "prepare": "npm run build",
    "start": "node build/server.js"
  },
  "dependencies": {
    "argv": "0.0.2",
    "bootstrap": "^4.1.3",
    "chart.js": "^2.7.2",
    "express": "^4.16.3",
    "moment": "^2.22.2",
    "mysql": "^2.16.0",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@types/argv": "0.0.4",
    "@types/bootstrap": "^4.1.2",
    "@types/chart.js": "^2.7.28",
    "@types/express": "^4.16.0",
    "@types/mysql": "^2.15.5",
    "@types/node": "^10.5.3",
    "@types/react": "^16.4.7",
    "@types/react-bootstrap": "^0.32.11",
    "@types/react-dom": "^16.0.6",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.4.2",
    "typescript": "^2.9.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0"
  }
}

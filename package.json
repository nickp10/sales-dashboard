{
  "name": "sales-dashboard",
  "version": "1.1.1",
  "description": "A dashboard built upon chart.js for displaying sales transactions",
  "bin": "build/server.js",
  "main": "build/server.js",
  "files": [
    "build/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickp10/sales-dashboard.git"
  },
  "author": "nickp10@hotmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nickp10/sales-dashboard/issues"
  },
  "homepage": "https://github.com/nickp10/sales-dashboard#readme",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-client": "webpack --config webpack.client.config.js --mode production",
    "build-server": "webpack --config webpack.server.config.js --mode production",
    "prepare": "npm run build",
    "start": "node build/server.js"
  },
  "dependencies": {
    "argv": "0.0.2",
    "chart.js": "^2.7.2",
    "express": "^4.16.3",
    "moment": "^2.22.0",
    "mysql": "^2.15.0",
    "react": "^16.3.0",
    "react-chartjs-2": "^2.7.0",
    "react-dom": "^16.3.0"
  },
  "devDependencies": {
    "@types/argv": "0.0.4",
    "@types/chart.js": "^2.7.10",
    "@types/express": "^4.11.1",
    "@types/mysql": "^2.15.4",
    "@types/node": "^9.6.1",
    "@types/react": "^16.3.1",
    "@types/react-dom": "^16.0.4",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^3.1.0",
    "ts-loader": "^4.1.0",
    "typescript": "^2.8.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13"
  }
}
